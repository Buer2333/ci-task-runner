{
  "modules": [],
  "librarys": ["package.json"],
  "assets": "dist/assets.json",
  "force": false,
  "repository": "git",
  "builder": {
    "command": "webpack --config ${modulePath}/webpack.config.js",
    "options": {
      "cwd": "${modulePath}",
      "env": {
        "$ASSETS_PATH": "${assetsPath}",
        "$MODULE_NAME": "${moduleName}",
        "$MODULE_PATH": "${modulePath}"
      },
      "timeout": 0
    }
  }
}