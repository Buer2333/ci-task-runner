{
  "modules": [],
  "dependencies": [],
  "assets": "dist/assets.json",
  "force": false,
  "repository": "git",
  "builder": {
    "command": "webpack --config ${modulePath}/webpack.config.js",
    "options": {
      "cwd": "${modulePath}",
      "env": {
        "MODULE_WATCHER": "1",
        "MODULE_WATCHER_MODULE_NAME": "${moduleName}",
        "MODULE_WATCHER_MODULE_PATH": "${modulePath}"
      },
      "timeout": 0
    }
  }
}