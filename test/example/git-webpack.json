{
  "modules": ["git-webpack-module-example"],
  "watch": ["package.json"],
  "assets": "dist/assets.json",
  "parallel": 2,
  "build": {
    "force": false,
    "timeout": 60000,
    "cwd": "${moduleName}",
    "env": {
      "GIT_WEBPACK": "1"
    },
    "argv": [],
    "launch": "${moduleName}/webpack.config.js"
  }
}