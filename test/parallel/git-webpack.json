{
  "modules-bk": ["git-webpack-module-example", "mod1"],
  "modules": ["git-webpack-module-example", "mod1", "mod2", "mod3", "mod4", "mod5", "mod6", "mod7", "mod8"],
  "watch": ["package.json"],
  "assets": "dist/assets.json",
  "parallel": 3,
  "build": {
    "force": true,
    "timeout": 60000,
    "cwd": "${moduleName}",
    "env": {
      "GIT_WEBPACK": "1"
    },
    "argv": [],
    "launch": "${moduleName}/webpack.config.js"
  }
}