{
  "modules": ["mod1"],
  "watch": ["package.json"],
  "assets": "dist/assets.json",
  "parallel": 3,
  "builder": {
    "force": false,
    "timeout": 60000,
    "cwd": "${moduleName}",
    "env": {
      "GIT_WEBPACK": "1"
    },
    "execArgv": [],
    "silent": true,
    "name": "webpack",
    "launch": "${moduleName}/webpack.config.js"
  }
}