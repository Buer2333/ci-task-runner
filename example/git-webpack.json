{
  "modules": ["git-webpack-module-example"],
  "librarys": ["package.json"],
  "assets": "dist/assets.json",
  "parallel": 3,
  "builder": {
    "force": false,
    "timeout": 60000,
    "cwd": "${moduleName}",
    "env": {
      "MODULE_NAME": "${moduleName}",
      "MODULE_COMMIT": "${moduleCommit}"
    },
    "execArgv": [],
    "silent": false,
    "name": "webpack",
    "launch": "${moduleName}/webpack.config.js"
  }
}