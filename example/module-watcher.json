{
  "modules": ["module-example"],
  "dependencies": ["package.json"],
  "assets": "dist/assets.json",
  "force": false,
  "repository": "git",
  "program": {
    "command": "webpack --color --config ${modulePath}/webpack.config.js",
    "options": {
      "env": {
        "MODULE_WATCHER_NAME": "${moduleName}",
        "MODULE_WATCHER_PATH": "${modulePath}"
      },
      "timeout": 0
    }
  }
}